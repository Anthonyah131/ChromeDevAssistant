{
    "manifest_version": 3,
    "name": "ChromeDevAssistant",
    "icons": {
        "32": "icon.png"
    },
    "version": "1.0.0",
    "background": {
      "service_worker": "background.js"
    },
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}